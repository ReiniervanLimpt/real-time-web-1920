<%- include('partials/head') %>

  <div id="gameWaiter">
    <h1>No game active!</h1>
    <p>please come back later</p>
  </div>
  <div id="nameSelector">
    <h1>Spectate my Game!</h1>
    <form class="username" action="">
      <input id="u" autocomplete="off" /><input id="send" type="submit" value="set username">
    </form>
  </div>
  <div>
    <section id="eventSection">
      <p class="eventTitle">EVENTS</p>
      <p class="teamChaosTitle">TEAM CHAOS</p>
      <p class="teamOrderTitle">TEAM ORDER</p>
      <ul id="events"></ul>
      <div id="championKills"></div>
      <div id="teamChaos">
      </div>
      <div id="teamOrder">
      </div>
    </section>
    <section id="chatSection">
      <ul id="messages"></ul>

      <form class="message" action="">
        <input id="m" autocomplete="off" /><input id="send" type="submit" value="send">
      </form>
    </section>
  </div>


  </main>

  <script src="/socket.io/socket.io.js"></script>
  <script type="module" src="/client.js"></script>